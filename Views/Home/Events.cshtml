@model OpportunitiesDraft1.ViewModels.NewsletterViewModel
@using OpportunitiesDraft1.Components

@{
    ViewBag.Title = "Events";
}



<div class="OpportunistHome">
    <div class="OpportunistHomeVideo">
        <div class="SupportersMainImage"><img src="/Images/Events.jpeg" class="img-sup" alt="event"></div>
        <div class="BameTextContainer mx-auto">
            <div class="m-5 p-2 SupporterMainText">
                <div class="card" style="background: #C0C0C0; font-size:18px; color:#47525e">
