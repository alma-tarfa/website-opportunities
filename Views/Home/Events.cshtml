                        <h5 class="card-title">Want to stay up to date with our latest news and events</h5>
                        <button class="btn btn-dark mt-4" onclick="location.href='@Url.Action("Newsletter", "home")'">
                            Subscribe to our NewsLetter
                        </button>
                    </div>
                </div>
            </div>

        </div>
    </div>
    @* @(await Html.RenderComponentAsync<Events>(RenderMode.ServerPrerendered))*@

</div>


