@{
    ViewBag.Title = "Create Account";
}

<div class="PageBody" style="position:relative; top:80px;">
    <div class="card mx-auto login-container">
        <div class="card-header text-center boldText">Register</div>
        <h5 class="card-title "></h5>
        <div class="card-body login-container">
            <button type="button" class="btn btn-outline-dark btn-lg btn-block" onclick="location.href='@Url.Action("RegisterPartner", "Account")'">I am a Partner</button>
            <button type="button" class="btn btn-outline-dark btn-lg btn-block" onclick="location.href='@Url.Action("RegisterOpportunist", "Account")'">I am an Opportunist</button>
        </div>
    </div>
</div>
